<h1>appFinder</h1>
<table class="table table-striped table-hover table-bordered">
  <tr>
    <th>
      Icon
    </th>
    <th>
      Name
    </th>
    <th>
      Category
    </th>
    <th>
      Price
    </th>
  </tr>
  {{#each rows}}
  <tr>
    <td>
      {{#if picture}}
        <img src = "/images/{{picture}}" width = "60" height = "60" style = "border-radius: 15px;">
        {{else}}
        No icon available yet
      {{/if}}
    </td>
    <td> {{name}}</td>
    <td>{{ category }}</td>
    <td>{{ price }}</td>
    
  </tr>
  {{/each}}
</table>
